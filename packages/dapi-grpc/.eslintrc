{
  "extends": "airbnb-base",
  "env": {
    "es2020": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "packageDir": "." }],
    "no-plusplus": 0,
    "eol-last": [
      "error",
      "always"
    ],
    "class-methods-use-this": "off",
    "curly": [
      "error",
      "all"
    ]
  }
}
