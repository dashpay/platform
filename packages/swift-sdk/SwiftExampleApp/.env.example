# Test Identity and Keys for State Transition Tests
# Copy this file to .env and add your actual keys

# Identity ID (base58)
TEST_IDENTITY_ID=YOUR_TEST_IDENTITY_ID_HERE

# Key 1: Critical Authentication Key
TEST_KEY_1_ID=1
TEST_KEY_1_PRIVATE=YOUR_PRIVATE_KEY_1_HERE_IN_WIF_FORMAT

# Key 3: Critical Transfer Key  
TEST_KEY_3_ID=3
TEST_KEY_3_PRIVATE=YOUR_PRIVATE_KEY_3_HERE_IN_WIF_FORMAT

# Recipient Identity for transfers (optional)
TEST_RECIPIENT_ID=RECIPIENT_IDENTITY_ID_HERE

# Test network (testnet/mainnet)
TEST_NETWORK=testnet