    "enquirer": "^2.3.6",
    "glob": "^7.1.6",
    "hasbin": "^1.2.3",
    "jayson": "^3.3.4",
    "listr2": "^3.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "memory-streams": "^0.1.3",
    "node-fetch": "^2.6.1",
    "node-graceful": "^3.0.0",
    "pretty-bytes": "^5.3.0",
    "pretty-ms": "^7.0.0",
    "public-ip": "^4.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "semver": "^7.3.2",
    "table": "^5.4.6"
