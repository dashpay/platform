    "@dashevo/feature-flags-contract": "~0.2.0",
    "@dashevo/wallet-lib": "github:dashevo/wallet-lib#update-dpp",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3.2.2",
    "ajv": "^6.12.3",
    "awilix": "^4.2.6",
    "bls-signatures": "^0.2.5",
