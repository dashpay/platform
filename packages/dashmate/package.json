    "globby": "^10.0.2"
  },
  "files": [
    "bin",
    "oclif.manifest.json",
    "src"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "dashmate",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "wallet": {
        "description": "Wallet related commands"
      },
      "status": {
        "description": "Show status details"
      }
    }
  }
}
