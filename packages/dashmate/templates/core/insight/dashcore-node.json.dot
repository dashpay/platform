{
  "network": "{{= it.network }}",
  "port": {{= it.core.insight.port }},
  "services": [
    "dashd",
    "@dashevo/insight-api",
    {{? it.core.insight.ui.enabled }}"@dashevo/insight-ui",{{?}}
    "web"
  ],
  "servicesConfig": {
    "dashd": {
      "connect": [{
        "rpchost": "core",
        "rpcport": {{= it.core.rpc.port }},
        "rpcuser": "dashmate",
        "rpcpassword": "{{= it.core.rpc.users.dashmate.password }}",
        "zmqpubrawtx": "tcp://core:{{= it.core.zmq.port }}",
        "zmqpubhashblock": "tcp://core:{{= it.core.zmq.port }}"
      }]
    },
    "@dashevo/insight-api": {
      "disableRateLimiter": true
    }
  }
}
